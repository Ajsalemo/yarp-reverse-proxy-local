{
  "Urls": "http://localhost:5000;https://localhost:5001",
  "LogLevel": {
    "Default": "Information",
    "Microsoft.AspNetCore": "Warning"
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "minimumroute": {
        "ClusterId": "minimumcluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "localCluster": {
        "Destinations": {
          "google.com": {
            "Address": "http://localhost:8080/"
          }
        }
      }
    }
  }
}
